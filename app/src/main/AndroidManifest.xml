<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tipan.databasing">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Databasingg"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.tipan.databasing.HomeActivity"></activity>
        <activity
            android:name="com.tipan.databasing.MainActivity"
            android:label="DATABASE"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name="com.tipan.databasing.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.tipan.databasing.SignupActivity" />
        <activity android:name="com.tipan.databasing.EdituserActivity"/>
    </application>

</manifest>